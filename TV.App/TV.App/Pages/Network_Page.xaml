<UserControl
    x:Class="TV.App.Pages.NetworkPage"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TV.App.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="clr-namespace:TV.App.UserControls"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    mc:Ignorable="d">
    <StackPanel MaxWidth="800" Margin="50,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
        <uc:TimeSetting Title="Enable Wifi" HasToggle="True" IsChecked="True" />
        <uc:TimeSetting Title="        Network SND-1" HasText="True" HasToggle="False" Text="...  " />
        <uc:TimeSetting Title="        Network SND-2" HasText="True" HasToggle="False" Text="...  " />
        <uc:TimeSetting Title="        Network SND-3" HasText="True" HasToggle="False" Text="...  " />
        <uc:TimeSetting Title="Enable Hotspot" HasText="True"/>
        <uc:TimeSetting Title="Mac Address"/>
    </StackPanel>
</UserControl>
